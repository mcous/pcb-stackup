{
  "version": "4.0.0-next.18",
  "command": {
    "version": {
      "conventionalCommits": true,
      "message": "chore(release): %s",
      "preid": "next",
      "push": "false"
    }
  },
  "ignoreChanges": [
    "**/*.md",
    "**/*test.js",
    "**/integration/**/*.js",
    "**/example/**/*.js",
    "**/scripts/**/*.js"
  ],
  "packages": ["apps/*", "packages/*"]
}
